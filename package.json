{
  "name": "artemis",
  "version": "0.1.2",
  "description": "Artemis - IDE",
  "author": {
    "name": "Foxemsx",
    "url": "https://github.com/Foxemsx"
  },
  "license": "MIT",
  "homepage": "https://github.com/Foxemsx/Artemis",
  "repository": {
    "type": "git",
    "url": "https://github.com/Foxemsx/Artemis.git"
  },
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:icons": "node scripts/generate-icons.js",
    "dist": "npm run build && npm run build:icons && electron-builder --config electron-builder.yml",
    "dist:win": "npm run build && npm run build:icons && electron-builder --win --config electron-builder.yml",
    "dist:mac": "npm run build && npm run build:icons && electron-builder --mac --config electron-builder.yml",
    "dist:linux": "npm run build && npm run build:icons && electron-builder --linux --config electron-builder.yml",
    "postinstall": "node scripts/rebuild-pty.js"
  },
  "dependencies": {
    "@monaco-editor/react": "4.7.0",
    "@xterm/addon-fit": "0.9.0",
    "@xterm/addon-web-links": "0.10.0",
    "@xterm/xterm": "5.4.0",
    "framer-motion": "10.18.0",
    "lucide-react": "0.312.0",
    "monaco-editor": "^0.52.2",
    "node-pty": "1.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "10.1.0",
    "react-resizable-panels": "4.6.2",
    "remark-gfm": "4.0.1",
    "safe-regex2": "^5.0.0",
    "tiktoken": "^1.0.22",
    "undici": "^7.21.0"
  },
  "browserslist": [
    "last 1 electron version"
  ],
  "devDependencies": {
    "@electron/rebuild": "3.7.1",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "10.4.17",
    "electron": "35.1.0",
    "electron-builder": "^25.1.8",
    "png2icons": "^2.0.1",
    "postcss": "8.4.33",
    "postcss-nesting": "^14.0.0",
    "sharp": "^0.33.5",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "vite": "5.0.11",
    "vite-plugin-electron": "0.28.8"
  }
}
